<scraperfunctions>
	<GetHDTrailer dest="5">
		<RegExp input="$$4" output="&lt;details&gt;&lt;trailer&gt;\1&lt;/trailer&gt;&lt;/details&gt;" dest="5">
			<RegExp input="$$1" output="http://playlist.yahoo.com/makeplaylist.dll?sdm=web&amp;pt=rd&amp;sid=\1" dest="4">
				<expression>sid=(\d*)"&gt;480p&lt;/a&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="http://playlist.yahoo.com/makeplaylist.dll?sdm=web&amp;pt=rd&amp;sid=\1" dest="4">
				<expression>sid=(\d*)"&gt;720p&lt;/a&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="http://playlist.yahoo.com/makeplaylist.dll?sdm=web&amp;pt=rd&amp;sid=\1" dest="4">
				<expression>sid=(\d*)"&gt;$INFO[hdtres]&lt;/a&gt;</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetHDTrailer>
</scraperfunctions>