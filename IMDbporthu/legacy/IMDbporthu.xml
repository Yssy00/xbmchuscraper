			<RegExp input="$$1" output="&lt;plot&gt;\1&lt;/plot&gt;" dest="5">
				<expression>&lt;div class="separator"&gt;&lt;/div&gt;\n&lt;span class="txt"&gt;(.*?)&lt;/span&gt;</expression>
