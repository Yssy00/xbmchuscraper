<?xml version="1.0" encoding="utf-8"?><scraper framework="1,1" date="2010-04-15" name="port.hu" content="movies" thumb="port.png" language="hu">
	<include>common/imdb.xml</include>
	<include>common/tmdb.xml</include>
	<include>common/movieposterdb.xml</include>
	<include>common/impa.xml</include>
	<include>common/dtrailer.xml</include>
	<GetSettings dest="3">
		<RegExp input="$$5" output="&lt;settings&gt;\1&lt;/settings&gt;" dest="3">
			<RegExp input="$$1" output="&lt;setting label=&quot;Magyar filmcím&quot; type=&quot;bool&quot; id=&quot;huntitle&quot; default=&quot;true&quot;&gt;&lt;/setting&gt;" dest="5">
				<expression />
			</RegExp>
			<RegExp input="$$1" output="&lt;setting label=&quot;Magyar besorolás&quot; type=&quot;bool&quot; id=&quot;hunmpaa&quot; default=&quot;true&quot;&gt;&lt;/setting&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp input="$$1" output="&lt;setting label=&quot;Teljes stáblista letöltése&quot; type=&quot;bool&quot; id=&quot;fullcredits&quot; default=&quot;false&quot;&gt;&lt;/setting&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp input="$$1" output="&lt;setting label=&quot;Fanart letöltése themoviedb.org-ról&quot; type=&quot;bool&quot; id=&quot;fanart&quot; default=&quot;true&quot;&gt;&lt;/setting&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp input="$$1" output="&lt;setting label=&quot;Borítók letöltése themoviedb.org-ról&quot; type=&quot;bool&quot; id=&quot;tmdbthumbs&quot; default=&quot;true&quot;&gt;&lt;/setting&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp input="$$1" output="&lt;setting label=&quot;Borítók letöltése IMPAwards-ról&quot; type=&quot;bool&quot; id=&quot;impawards&quot; default=&quot;false&quot;&gt;&lt;/setting&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp input="$$1" output="&lt;setting label=&quot;Borítók letöltése MoviePosterDB-ról&quot; type=&quot;bool&quot; id=&quot;movieposterdb&quot; default=&quot;false&quot;&gt;&lt;/setting&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp input="$$1" output="&lt;setting label=&quot;Filmelőzetes Dtrailer.com-ról&quot; type=&quot;bool&quot; id=&quot;dtrailer&quot; default=&quot;false&quot;&gt;&lt;/setting&gt;" dest="5+">
				<expression />
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetSettings>
	<CreateSearchUrl SearchStringEncoding="iso-8859-2" dest="3">
		<RegExp input="$$1" output="http://www.port.hu/pls/ci/cinema.film_creator?i_text=\1&amp;i_film_creator=1" dest="3">
			<expression clear="yes" noclean="1" />
		</RegExp>
	</CreateSearchUrl>
	<GetSearchResults dest="8">
		<RegExp input="$$5" output="&lt;?xml version=&quot;1.0&quot; encoding=&quot;iso-8859-2&quot; standalone=&quot;yes&quot;?&gt;&lt;results&gt;\1&lt;/results&gt;" dest="8">
			<RegExp input="$$1" output="\1" dest="7">
				<expression clear="yes">i_film_id=([0-9]*)&amp;</expression>
			</RegExp>
			<RegExp input="$$1" output="\1" dest="9">
				<expression clear="yes">, (19\d\d|20\d\d)&lt;/span&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;entity&gt;&lt;title&gt;\1 \3&lt;/title&gt;&lt;year&gt;$$9&lt;/year&gt;&lt;url&gt;http://www.port.hu/pls/fi/films.film_page?i_where=2&amp;i_film_id=$$7&lt;/url&gt;&lt;id&gt;$$7&lt;/id&gt;&lt;/entity&gt;" dest="5">
				<expression clear="yes" noclean="1">&lt;span class="blackbigtitle"&gt;([^&lt;]*)(&lt;/span&gt;\n&lt;br/&gt;&lt;span class="txt"&gt;([^&lt;]*))?</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;entity&gt;&lt;title&gt;\2 \3&lt;/title&gt;&lt;year&gt;\5&lt;/year&gt;&lt;url&gt;http://www.port.hu/pls/fi/films.film_page?i_where=2&amp;i_film_id=\1&lt;/url&gt;&lt;/entity&gt;" dest="5">
				<expression repeat="yes" noclean="1">i_film_id=([\d]*)&amp;amp;i_city_id[^&gt;]*.([^&lt;]*)[^&gt;]*.([^&lt;]*)(.*?)(\d{4})?\) &lt;</expression>
			</RegExp>
			<expression clear="yes" noclean="1" />
		</RegExp>
	</GetSearchResults>
	<GetDetails dest="3">
		<RegExp input="$$5" output="&lt;?xml version=&quot;1.0&quot; encoding=&quot;iso-8859-2&quot; standalone=&quot;yes&quot;?&gt;&lt;details&gt;\1&lt;/details&gt;" dest="3">
			<RegExp conditional="huntitle" input="$$1" output="&lt;title&gt;\1&lt;/title&gt;" dest="5">
				<expression>&lt;span class="blackbigtitle"&gt;([^&lt;]*)</expression>
			</RegExp>
			<RegExp conditional="!huntitle" input="$$1" output="&lt;title&gt;\3&lt;/title&gt;" dest="5">
				<expression>&lt;span class="blackbigtitle"&gt;([^&lt;]*)(&lt;/span&gt;\n&lt;br/&gt;&lt;span class="txt"&gt;\(([^\)]*))?</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;plot&gt;\1&lt;/plot&gt;" dest="5+">
				<expression>&lt;div class="separator"&gt;&lt;/div&gt;\n&lt;span class="txt"&gt;(.*?)&lt;/span&gt;</expression>
			</RegExp>
			<!--   -->
			<RegExp input="$$1" output="&lt;outline&gt;\1&lt;/outline&gt;" dest="5+">
				<expression>&lt;div class="separator"&gt;&lt;/div&gt;\n&lt;span class="txt"&gt;(.*?[^A-Z]\.)</expression>
			</RegExp>
			<!--(\d{2})-->
			<RegExp conditional="hunmpaa" input="$$1" output="&lt;mpaa&gt;\1 \2&lt;/mpaa&gt;" dest="5+">
				<expression>\d{4}&lt;/span&gt;&lt;/span&gt;&lt;br/&gt;&lt;span class="btxt"&gt;(\d{2})(.*?)&amp;nbsp;&lt;/span&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="\1" dest="14">
				<expression>, (19\d\d|20\d\d)&lt;/span&gt;</expression>
			</RegExp>
			<RegExp input="$$14" output="&lt;year&gt;\1&lt;/year&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp input="$$1" output="&lt;director&gt;\2&lt;/director&gt;" dest="5+">
				<expression>rendez.:&amp;nbsp;&lt;/span&gt;&lt;span class="txt"&gt;&lt;a(.*?)target="_top"&gt;(.*?)&lt;/a&gt;&lt;/span&gt;</expression>
			</RegExp>
			<RegExp input="$$10" output="\1" dest="5+">
				<RegExp input="$$9" output="&lt;url function=&quot;GetIMDBDetails&quot;&gt;http://akas.imdb.com/title/tt\1/&lt;/url&gt;" dest="10">
					<RegExp input="$$1" output="\1" dest="9">
						<expression clear="yes">i_param=([0-9]*)" target="top"&gt;IMDb&lt;/a&gt;&lt;/span&gt;</expression>
					</RegExp>
					<expression />
				</RegExp>
				<expression noclean="1" />
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetDetails>
	<GetIMDBDetails clearbuffers="no" dest="3">
		<RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="3">
			<!--IMDB ID-->
			<RegExp input="$$1" output="\1" dest="11">
				<expression>/title/tt([t0-9]*)/faq</expression>
			</RegExp>
			<!--IMDB URL-->
			<RegExp input="$$1" output="http://www.imdb.com/title/tt\1/" dest="12">
				<expression>/title/tt([t0-9]*)/faq</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;runtime&gt;\1&lt;/runtime&gt;" dest="5">
				<expression trim="1">&lt;h5&gt;Runtime:&lt;/h5&gt;[^0-9]*([^&lt;]*)</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;top250&gt;\1&lt;/top250&gt;" dest="5+">
				<expression>Top 250: #([0-9]*)&lt;/a&gt;</expression>
			</RegExp>
			<RegExp conditional="!hunmpaa" input="$$1" output="&lt;mpaa&gt;\1&lt;/mpaa&gt;" dest="5+">
				<expression>MPAA&lt;/a&gt;:&lt;/h5&gt;\n&lt;div class="info-content"&gt;\n(.[^&lt;]*)</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;tagline&gt;\1&lt;/tagline&gt;" dest="5+">
				<expression>&lt;h5&gt;Tagline:&lt;/h5&gt;\n&lt;div class="info-content"&gt;([^&lt;]*)</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;rating&gt;\1&lt;/rating&gt;&lt;votes&gt;\2&lt;/votes&gt;" dest="5+">
				<expression>&lt;b&gt;([0-9.]+)/10&lt;/b&gt;[^&lt;]*&lt;a href="ratings" class="tn15more"&gt;([0-9,]+) votes&lt;/a&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;genre&gt;\1&lt;/genre&gt;" dest="5+">
				<expression>"/Sections/Genres/[^/]*/"&gt;([^&lt;]*)&lt;/a&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;studio&gt;\1&lt;/studio&gt;" dest="5+">
				<expression>"/company/[^/]*/"&gt;([^&lt;]*)&lt;/a&gt;</expression>
			</RegExp>
			<RegExp conditional="!fullcredits" input="$$11" output="&lt;url cache=&quot;tt$$11-credits.html&quot; function=&quot;GetIMDBCast&quot;&gt;$$12&lt;/url&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp conditional="!fullcredits" input="$$11" output="&lt;url cache=&quot;tt$$11-credits.html&quot; function=&quot;GetIMDBDirectors&quot;&gt;$$12&lt;/url&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp conditional="!fullcredits" input="$$11" output="&lt;url cache=&quot;tt$$11-credits.html&quot; function=&quot;GetIMDBWriters&quot;&gt;$$12&lt;/url&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp conditional="fullcredits" input="$$11" output="&lt;url cache=&quot;tt$$11-fullcredits.html&quot; function=&quot;GetIMDBCast&quot;&gt;$$12fullcredits&lt;/url&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp conditional="fullcredits" input="$$11" output="&lt;url cache=&quot;tt$$11-fullcredits.html&quot; function=&quot;GetIMDBDirectors&quot;&gt;$$12fullcredits&lt;/url&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp conditional="fullcredits" input="$$11" output="&lt;url cache=&quot;tt$$11-fullcredits.html&quot; function=&quot;GetIMDBWriters&quot;&gt;$$12fullcredits&lt;/url&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp conditional="tmdbthumbs" input="$$1" output="&lt;url function=&quot;GetTMDBThumbsById&quot;&gt;$$12&lt;/url&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp conditional="fanart" input="$$1" output="&lt;url function=&quot;GetTMDBFanartById&quot;&gt;$$12&lt;/url&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp conditional="movieposterdb" input="$$1" output="&lt;url function=&quot;GetMoviePosterDBLink&quot;&gt;http://www.movieposterdb.com/browse/search?type=movies&amp;amp;query=$$11&lt;/url&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp conditional="impawards" input="$$11" output="&lt;url cache=&quot;tt$$11-posters.html&quot; function=&quot;GetIMPALink&quot;&gt;$$12posters&lt;/url&gt;" dest="5+">
				<expression />
			</RegExp>
			<RegExp conditional="dtrailer" input="$$6" output="&lt;url function=&quot;GetDTrailerLink&quot;&gt;http://en.dtrailer.com/movies/search/\1&lt;/url&gt;" dest="5+">
				<RegExp input="$$4" output="\1-" dest="6">
					<RegExp input="$$1" output="\1" dest="4">
						<expression trim="1" noclean="1">&lt;h1&gt;([^&lt;]*)</expression>
					</RegExp>
					<expression repeat="yes">([a-zA-Z0-9]+)</expression>
				</RegExp>
				<expression />
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetIMDBDetails>
</scraper>